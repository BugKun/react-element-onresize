{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "chrome > 55"
                    ]
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "react-scope-binding",
        "transform-decorators-legacy",
        "add-module-exports",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-export-extensions",
        "transform-class-properties",
        ["react-css-modules", {
            "generateScopedName": "[local]-v-[hash:8]",
            "filetypes": {
                ".scss": {
                    "syntax": "postcss-scss"
                }
            }
        }]
    ]
}
